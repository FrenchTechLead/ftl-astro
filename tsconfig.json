{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@comps/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "public/assets/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}